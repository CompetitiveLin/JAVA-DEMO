            <result property="dissertationKeywords" column="dissertation_keywords" jdbcType="VARCHAR"/>
            <result property="dissertationAbstract" column="dissertation_abstract" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,stu_id,information_confirmation,
        information_confirmation_time,course_confirmation,course_confirmation_time,
        academic_confirmation,academic_confirmation_time,dissertation_submitted,
