<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mbg.mapper.UserConfirmationMapper">

    <resultMap id="BaseResultMap" type="com.example.demo.mbg.model.UserConfirmation">
            <id property="id" column="id" jdbcType="BIGINT"/>
            <result property="stuId" column="stu_id" jdbcType="BIGINT"/>
            <result property="informationConfirmation" column="information_confirmation" jdbcType="TINYINT"/>
            <result property="informationConfirmationTime" column="information_confirmation_time" jdbcType="TIMESTAMP"/>
            <result property="courseConfirmation" column="course_confirmation" jdbcType="TINYINT"/>
